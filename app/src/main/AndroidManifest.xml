<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a14512.discover">

    <application
        android:name=".DiscoverApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".modules.main.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modules.travel.view.TravelKnowledgeActivity" />
        <activity android:name=".modules.arround.view.AroundActivity" />
        <activity android:name=".modules.community.view.CommunityActivity" />
        <activity android:name=".modules.route.view.RouteActivity" />
        <activity android:name=".modules.main.view.activity.ChooseActivity" />
    </application>

</manifest>